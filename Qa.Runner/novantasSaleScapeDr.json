          },
          "style": {
            "indent": 5
          }
        },
        {
          "field": "TRXN_TYPE",
          "title": "Trxn Type",
          "calculation": {
            "type": "sum",
            "field": "TRXN_COUNT",
            "group": true
          },
          "sort": "numeric",
          "style": {
            "indent": 4
          }
        },
        {
          "field": "TRXN_COUNT",
          "title": "Trxn Count",
          "style": {
            "indent": 5
          }
        }
      ]
    }
  }
]