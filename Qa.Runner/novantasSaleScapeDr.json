[
  {
    "name": "Account Level Data",
    "fileFields": [
      { "name": "YEAR_MONTH" },
      { "name": "CUST_ID" },
      { "name": "CUST_COST_CENTER" },
      { "name": "CUST_SINCE_DATE" },
      { "name": "HH_ID" },
      { "name": "HH_COST_CENTER" },
      { "name": "HH_SINCE_DATE" },
      { "name": "ACCT_ID" },
      { "name": "ORIG_COST_CENTER" },
      { "name": "ACCT_COST_CENTER" },
      { "name": "ACCT_OPEN_DATE" },
      { "name": "ACCT_CLOSE_DATE" },
      { "name": "DATE_RENEWED" },
      { "name": "LOB" },
      { "name": "PROD_CODE1" },
      { "name": "PROD_CODE2" },
      { "name": "PROD_CODE3" },
      {
        "name": "TERM",
        "type": "Numeric"
      },
      {
        "name": "BAL_EOM",
        "numberFormat": "Money"
      },
      {
        "name": "AVG_BAL",
        "numberFormat": "Money"
      },
      {
        "name": "LINE_EOM",
        "numberFormat": "Money"
      },
      {
        "name": "INTEREST_RATE",
        "type": "Numeric"
      },
      {
        "name": "FEE_INC",
        "numberFormat": "Money"
      }
    ],
    "qa": {
      "fileMask": "ACCT*.txt",
      "rowsInHeader": 1,
      "delimiter": "|",
      "fields": [
        {
          "field": "CUST_ID",
          "title": "Customer ID",
          "calculation": "countUnique",
          "style": {
            "indent": 1
          }
        },
        {
          "field": "CUST_COST_CENTER",
          "title": "Customer Cost Center",
          "calculation": "countUnique",
          "style": {
            "indent": 1
          }
        },
        {
          "field": "HH_ID",
          "title": "Household ID",
          "calculation": "countUnique",
          "style": {
            "indent": 1
          }
        },
        {
          "field": "HH_COST_CENTER",
          "title": "Household Cost Center",
          "calculation": "countUnique",
          "style": {
            "indent": 1
          }
        },
        {
          "field": "ACCT_ID",
          "title": "Account ID",
          "calculation": "countUnique",
          "style": {
            "indent": 1
          }
        },
        {
          "field": "ORIG_COST_CENTER",
          "title": "Original Cost Center",
          "calculation": "countUnique",
          "style": {
            "indent": 1
          }
        },
        {
          "field": "ACCT_COST_CENTER",
          "title": "Account Cost Center",
          "calculation": "countUnique",
          "style": { "indent": 1 }
        },
        {
          "groupBy": "LOB",
          "title": "Line of Business",
          "calculation": "count",
          "style": {
            "indent": 5
          }
        },
        {
          "groupBy": "PROD_CODE1",
          "title": "Product Code 1",
          "calculation": "count",
          "style": {
            "alignment": "center"
          }
        },
        {
          "title": "CD Term",
          "calculation": "count",
          "groupBy": "TERM",
          "bins": {
            "ranges": [
              { "hide": true },
              {
                "upto": "0",
                "name": "No term provided"
              },
              {
                "upto": "3",
                "name": "1-3 months"
              },
              {
                "upto": "6",
                "name": "4-6 months"
              },
              {
                "upto": "9",
                "name": "7-9 months"
              },
              {
                "upto": "12",
                "name": "10-12 months"
              },
              {
                "upto": "18",
                "name": "13-18 months"
              },
              {
                "upto": "24",
                "name": "19-24 months"
              },
              {
                "upto": "36",
                "name": "25-36 months"
              },
              {
                "upto": "48",
                "name": "37-48 months"
              },
              {
                "upto": "60",
                "name": "49-60 months"
              },
              {
                "from": "61",
                "to": "99999",
                "name": "60+ months"
              }
            ]
          },
          "style": {
            "indent": 3
          }
        },
        {
          "field": "BAL_EOM",
          "title": "End of Month Balance",
          "style": {
            "indent": 1
          }
        },
        {
          "field": "AVG_BAL",
          "title": "Avg. Monthly Balance",
          "style": {
            "indent": 1
          }
        },
        {
          "field": "LINE_EOM",
          "title": "Line End of Month",
          "style": {
            "indent": 1
          }
        },
        {
          "field": "FEE_INC",
          "title": "Fee Income",
          "style": {
            "indent": 1
          }
        }
      ]
    }
  },
  {
    "name": "ATM Transactions",
    "fileFields": [
      { "name": "YEAR_MONTH" },
      { "name": "ATM_ID" },
      { "name": "TRXN_TYPE" },
      {
        "name": "TRXN_COUNT",
        "type": "Numeric"
      }
    ],
    "qa": {
      "fileMask": "ATM_TRXN*.txt",
      "rowsInHeader": 1,
      "delimiter": "|",
      "fields": [
        {
          "field": "ATM_ID",
          "title": "ATM ID",
          "calculation": "countUnique",
          "style": {
            "indent": 6
          }
        },
        {
          "groupBy": "TRXN_TYPE",
          "field": "TRXN_COUNT",
          "title": "Trxn Type",
          "calculation": "sum",
          "style": {
            "indent": 4
          }
        },
        {
          "field": "TRXN_COUNT",
          "title": "Trxn Count",
          "style": {
            "indent": 5
          }
        }
      ]
    }
  },
  {
    "name": "Branch Customer Facing Trxn",
    "fileFields": [
      { "name": "YEAR_MONTH" },
      { "name": "BRANCH_ID" },
      { "name": "TRXN_TYPE" },
      {
        "name": "TRXN_COUNT",
        "type": "Numeric"
      }
    ],
    "qa": {
      "fileMask": "TRXN*.txt",
      "rowsInHeader": 1,
      "delimiter": "|",
      "fields": [
        {
          "field": "BRANCH_ID",
          "title": "Branch ID",
          "calculation": "countUnique",
          "style": {
            "indent": 5
          }
        },
        {
          "groupBy": "TRXN_TYPE",
          "title": "Trxn Type",
          "field": "TRXN_COUNT",
          "calculation": "sum",
          "style": {
            "indent": 4
          }
        },
        {
          "field": "TRXN_COUNT",
          "title": "Trxn Count",
          "style": {
            "indent": 5
          }
        }
      ]
    }
  }
]